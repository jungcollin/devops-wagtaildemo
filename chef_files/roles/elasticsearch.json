{
    "json_class": "Chef::Role",
    "run_list": [
        "recipe[java]",
        "recipe[elasticsearch]",
        "recipe[elasticsearch-setup]"
    ],
    "default_attributes": {
        "java": {
            "ark_timeout": 120099,
            "ark_download_timeout": 120099,
            "install_flavor": "oracle",
            "jdk_version": "7",
            "oracle": {
                "accept_oracle_download_terms": true
            }
        }
    }
}
